@using WebApplication7.Models
@model Usuario
<form method="post">
    <div class="form-group row">
        <label for="id" class="col-4 col-form-label">Id</label>
        <div class="col-8">
            <input id="IdUsuario" name="IdUsuario" type="text" value="@Model.IdUsuario" class="form-control">
        </div>
    </div>
    <div class="form-group row">
        <label for="text" class="col-4 col-form-label">Text Field</label>
        <div class="col-8">
            <input id="Clave" name="Clave" type="text" value="@Model.Clave" class="form-control">
        </div>
    </div>
    <div class="form-group row">
        <div class="offset-4 col-8">
            <button name="submit" type="submit" class="btn btn-primary">Submit</button>
        </div>
    </div>
</form>
